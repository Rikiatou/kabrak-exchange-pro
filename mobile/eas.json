{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "bogo-preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_NAME": "BOGO EXPRESS & SAOUDIE SERVICE",
        "APP_SLUG": "bogo-express-exchange",
        "APP_BUNDLE_ID": "com.bogoexpress.exchange",
        "APP_PACKAGE": "com.bogoexpress.exchange",
        "APP_SCHEME": "bogo-exchange",
        "APP_PRIMARY_COLOR": "#1a237e",
        "APP_SPLASH_BG": "#0d1447"
      }
    },
    "bogo-production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "APP_NAME": "BOGO EXPRESS & SAOUDIE SERVICE",
        "APP_SLUG": "bogo-express-exchange",
        "APP_BUNDLE_ID": "com.bogoexpress.exchange",
        "APP_PACKAGE": "com.bogoexpress.exchange",
        "APP_SCHEME": "bogo-exchange",
        "APP_PRIMARY_COLOR": "#1a237e",
        "APP_SPLASH_BG": "#0d1447"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-key.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "kabrakeng@gmail.com",
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
        "appleTeamId": "YOUR_APPLE_TEAM_ID"
      }
    }
  }
}
